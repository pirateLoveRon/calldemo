<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.calldemo.mapper.TabEmployeeInfoMapper">
    <select id="findListByEmployeeName" resultType="com.demo.calldemo.entity.TabEmployeeInfo">
        SELECT *
        FROM tab_employee_info
        WHERE employee_name LIKE CONCAT('%', #{employeeName}, '%')
    </select>
</mapper>